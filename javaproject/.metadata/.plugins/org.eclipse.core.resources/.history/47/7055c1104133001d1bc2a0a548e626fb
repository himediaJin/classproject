package chapter02;

public class ValiableCatingOperator {

	public static void main(String[] args) {
		
		
		char c1 = 'a';
		
		int result = c1 + 10; // char +  int => int + int => ê²°ê³¼ int
		
		System.out.println(result);
		
		int num1 = 10;
		float num2 = 10.0f;
		
		int result2 = num1 + num2; // int + float => float + float => float 
		
		
		
	}
	
	
}
